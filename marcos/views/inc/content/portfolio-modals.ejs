  <!-- Portfolio Modals -->
  <% portfolio.forEach((row)=>{%>
    <div class="portfolio-modal mfp-hide" id="portfolio-modal-<%= row.id %>">
      <div class="portfolio-modal-dialog bg-white">
        <a class="close-button d-none d-md-block portfolio-modal-dismiss" href="#">
          <i class="fa fa-3x fa-times"></i>
        </a>
        <div class="container text-center">
          <div class="row">
            <div class="col-lg-8 mx-auto">
              <h2 class="text-secondary text-uppercase mb-0"><%= row.title %></h2>
              <hr class="star-dark mb-5">
              <img class="img-fluid mb-5" src="<%= row.photo %>" alt="<%= row.title %>">
              <p class="mb-5"><%= row.description %></p>
              <a class="btn btn-primary btn-lg rounded-pill portfolio-modal-dismiss" href="#">
                <i class="fa fa-close"></i>
                Fechar</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  <% }); %>